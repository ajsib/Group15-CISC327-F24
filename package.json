{
	"name": "your-project-name",
	"version": "1.0.0",
	"description": "Your project description",
	"scripts": {
		"clean": "rm -rf coverage",
		"test": "nyc mocha --reporter spec --exit server/__tests__/*.mjs",
		"coverage": "nyc report --reporter=text --reporter=html"
	},
	"devDependencies": {
		"chai": "^5.1.2",
		"mocha": "^10.8.2",
		"nyc": "^17.1.0",
		"supertest": "^6.1.3"
	},
	"nyc": {
		"include": [
			"server/**/*.js"
		],
		"exclude": [
			"**/node_modules/**",
			"server/__tests__/*.mjs"
		],
		"reporter": [
			"text",
			"html"
		],
		"all": true,
		"extension": [
			".js"
		],
		"sourceMap": false,
		"instrument": true
	},
	"dependencies": {
		"cors": "^2.8.5",
		"cypress": "^13.16.0",
		"dotenv": "^16.4.5",
		"express": "^4.21.1",
		"mongoose": "^8.8.1"
	}
}
